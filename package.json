{
  "name": "online-shop",
  "version": "0.0.1",
  "description": "A full-stack e-commerce web application inspired by Digikala, built with TypeScript, NestJS, and React. This project demonstrates modern web development practices, including RESTful API design, TypeScript integration, and database management with Prisma and SQLite. It includes core e-commerce features such as product management, user authentication, and a shopping cart.",
  "author": "Sobhan-SRZA (mr.sinre) from Persian-Caesar",
  "private": false,
  "license": "BSD-3-Clause",
  "scripts": {
    "dev:backend": "cd backend && npm run start:dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm run start",
    "start:frontend": "cd frontend && npm run build",
    "start:dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
